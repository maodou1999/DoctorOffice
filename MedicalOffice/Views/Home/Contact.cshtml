    ViewBag.Title = "Contact";
}

<h2>Contact</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Address)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DisplayAddress)
        </dd>



        <dt>
            @Html.DisplayNameFor(model => model.Telphone)
        </dt>

        <dd>
